  "introduced_at": "1991-05-16", 
  "number": "155", 
  "official_title": "To establish a congressional question period for members of the President's Cabinet on the first Tuesday of each month during the present Congress.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
  "sponsor": {
    "district": "2", 
    "name": "Gejdenson, Sam", 
    "state": "CT", 
