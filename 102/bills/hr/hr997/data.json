  "introduced_at": "1991-02-20", 
  "number": "997", 
  "official_title": "To amend the Solid Waste Disposal Act to require a refund value for certain beverage containers, and to provide resources for State pollution prevention and recycling programs, and for other purposes.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": "National Recycling Resource Act", 
  "sponsor": {
    "district": "5", 
    "name": "Henry, Paul B.", 
