  "introduced_at": "1991-05-02", 
  "number": "2199", 
  "official_title": "To direct the Secretary of Veterans Affairs to conduct a pilot program to provide hospice care for terminally ill veterans.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
  "sponsor": {
    "district": null, 
    "name": "Dorgan, Byron L.", 
    "state": "ND", 
