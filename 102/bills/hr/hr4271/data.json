  "introduced_at": "1992-02-19", 
  "number": "4271", 
  "official_title": "To reform the system under which compensation for overtime customs inspectional services is determined; to amend chapters 83 and 84 of title 5, United States Code, to provide that customs inspectors and canine enforcement officers be treated as law enforcement officers for purposes of those chapters; and for other purposes.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
  "sponsor": {
    "district": "16", 
    "name": "Rangel, Charles B.", 
    "state": "NY", 
    "thomas_id": "00944", 
    "title": "Rep", 
    "type": "person"
  }, 
  "status": "REFERRED", 
  "status_at": "1992-02-19", 
  "subjects": [
    "Civil service retirement", 
    "Commuting", 
    "Customs administration", 
    "Federal employees", 
    "Foreign trade and international finance", 
