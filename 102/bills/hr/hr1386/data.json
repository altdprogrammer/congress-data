  "introduced_at": "1991-03-12", 
  "number": "1386", 
  "official_title": "To amend title 5, United States Code, to provide for the reimbursement of expenses incurred by a Federal employee in the adoption of a child.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
  "sponsor": {
    "district": "1", 
    "name": "Schroeder, Patricia", 
    "state": "CO", 
    "thomas_id": "01033", 
    "title": "Rep", 
    "type": "person"
  }, 
  "status": "REFERRED", 
  "status_at": "1991-03-12", 
  "subjects": [
    "Adoption", 
