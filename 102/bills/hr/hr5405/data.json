  "introduced_at": "1992-06-16", 
  "number": "5405", 
  "official_title": "To amend the Foreign Assistance Act of 1961 to ensure that United States cash transfer assistance is utilized to purchase United States goods and services, and for other purposes.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": "Foreign Aid Reform Act of 1992", 
  "sponsor": {
    "district": "9", 
    "name": "Torricelli, Robert G.", 
