  "introduced_at": "1991-08-01", 
  "number": "3166", 
  "official_title": "To amend the Internal Revenue Code of 1986 to increase the exclusion from gross income for combat pay received by a commissioned officer to $2,000 per month.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
  "sponsor": {
    "district": "6", 
