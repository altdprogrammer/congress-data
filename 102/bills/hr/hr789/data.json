  "introduced_at": "1991-02-04", 
  "number": "789", 
  "official_title": "To amend title 10, United States Code, to require the Armed Forces to provide not less than 10 minutes of free telephone calls a month for a member of the Armed Forces serving in a combat zone.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
