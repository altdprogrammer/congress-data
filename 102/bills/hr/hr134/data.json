  "number": "134", 
  "official_title": "To amend the Internal Revenue Code of 1986 with respect to the tax treatment of payments under life insurance contracts to terminally ill individuals.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
  "sponsor": {
    "district": "1", 
    "name": "Kennelly, Barbara B.", 
