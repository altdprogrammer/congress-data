  "introduced_at": "1991-02-06", 
  "number": "945", 
  "official_title": "To amend the Solid Waste Disposal Act to require States to provide an opportunity for their citizens to participate in comprehensive recycling programs, and for other purposes.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": "Comprehensive Recycling Act of 1991", 
  "sponsor": {
    "district": "3", 
