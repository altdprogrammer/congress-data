  "introduced_at": "1991-01-30", 
  "number": "722", 
  "official_title": "To amend title 32, United States Code, to provide that the protections afforded to Federal employees under subchapter II of chapter 75 of title 5, United States Code, be extended to National Guard technicians.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
  "sponsor": {
    "district": "31", 
    "name": "Dymally, Mervyn M.", 
