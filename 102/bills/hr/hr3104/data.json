  "introduced_at": "1991-07-30", 
  "number": "3104", 
  "official_title": "To amend title 18, United States Code, to prohibit the importation and the manufacture of firearms designed to accept a silencer, bayonet, grenade launcher, flash suppressor, or folding stock, of certain ammunition feeding devices, and of related devices, and to provide for the imposition of enhanced penalties for the possession or the use of any such item in a crime of violence or in a drug trafficking crime.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": "Assault Weapon Act  of 1991", 
  "sponsor": {
    "district": "9", 
    "name": "Stark, Fortney Pete", 
    "state": "CA", 
    "thomas_id": "01101", 
    "title": "Rep", 
    "type": "person"
  }, 
  "status": "REFERRED", 
  "status_at": "1991-07-30", 
  "subjects": [
    "Ammunition", 
    "Assault weapons", 
    "Crime and law enforcement", 
