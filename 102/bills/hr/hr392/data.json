  "introduced_at": "1991-01-03", 
  "number": "392", 
  "official_title": "To amend the Public Health Service Act to provide clarification with respect to the authority of grantees under title X of such Act to provide information and counseling regarding family planning.", 
  "popular_title": "Family Planning (Title X/Gag Order) bill", 
  "related_bills": [], 
  "short_title": "Title X Pregnancy Counseling Act of 1990", 
  "sponsor": {
    "district": "10", 
    "name": "Porter, John Edward", 
    "state": "IL", 
    "thomas_id": "00923", 
    "title": "Rep", 
    "type": "person"
  }, 
  "status": "REFERRED", 
  "status_at": "1991-01-03", 
  "subjects": [
    "Abortion", 
    "Adoption", 
    "Birth control", 
    "Counseling", 
    "Foster home care", 
    "Grants-in-aid", 
    "Health", 
    "Health education", 
