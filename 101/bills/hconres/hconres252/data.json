  "introduced_at": "1990-01-30", 
  "number": "252", 
  "official_title": "Expressing the policy of the Congress regarding proposals to reduce the cost-of-living adjustment for Civil Service retirees.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
  "sponsor": {
    "district": "20", 
    "name": "Oakar, Mary Rose", 
    "state": "OH", 
    "thomas_id": "00875", 
    "title": "Rep", 
    "type": "person"
  }, 
  "status": "REFERRED", 
  "status_at": "1990-01-30", 
  "subjects": [
    "Civil service pensions", 
    "Cost of living adjustments", 
    "Federal budgets", 
    "Federal employees"
  ], 
  "subjects_top_term": "Federal employees", 
  "summary": {
    "as": "Introduced", 
    "date": "1990-01-30", 
    "text": "Expresses the sense of the Congress that: (1) it would be inequitable for Civil Service retirees to receive smaller cost-of-living adjustments to their pensions than Social Security retirement beneficiaries; and (2) any budget adopted by the Congress should provide for full cost-of-living adjustments for all individuals receiving a Federal retirement pension or benefit."
  }, 
