  "status": "REFERRED", 
  "status_at": "1990-03-01", 
  "subjects": [
    "Aid to dependent children", 
    "Critically ill", 
    "Income tax", 
    "Life insurance", 
    "Medicaid", 
    "Public assistance programs", 
