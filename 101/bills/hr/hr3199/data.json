  "introduced_at": "1989-08-04", 
  "number": "3199", 
  "official_title": "To amend title 38, United States Code, to establish a program to provide post-secondary educational assistance to students in health professions who are eligible for educational assistance under the Reserve GI Bill program in return for agreement for subsequent service with the Department of Veterans Affairs.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": "Veterans Health Professionals Educational Amendments of 1989", 
  "sponsor": {
    "district": "4", 
    "name": "Smith, Christopher H.", 
    "state": "NJ", 
