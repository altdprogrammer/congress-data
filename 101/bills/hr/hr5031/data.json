  "introduced_at": "1990-06-13", 
  "number": "5031", 
  "official_title": "To amend the Internal Revenue Code of 1986 and the Federal Election Campaign Act of 1971 to make Federal elections more competitive, open, and honest by strengthening political parties, and for other purposes.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
  "sponsor": {
    "district": "18", 
    "name": "Michel, Robert H.", 
    "state": "IL", 
