  "introduced_at": "1989-04-12", 
  "number": "1816", 
  "official_title": "To amend the Older Americans Community Service Employment Act to authorize grants for the employment of older Americans in providing child care for families with low income.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": "Older Americans Child Care Employment Amendments of 1989", 
  "sponsor": {
    "district": "14", 
    "name": "Guarini, Frank J.", 
    "state": "NJ", 
    "thomas_id": "00472", 
