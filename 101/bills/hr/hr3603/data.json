  "introduced_at": "1989-11-07", 
  "number": "3603", 
  "official_title": "To direct the heads of Federal departments and agencies to disclose information concerning United States personnel classified as prisoners of war or missing in action from World War II, the Korean Conflict, and the Vietnam Conflict.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
  "sponsor": {
    "district": "5", 
    "name": "Smith, Denny", 
    "state": "OR", 
    "thomas_id": "01072", 
    "title": "Rep", 
