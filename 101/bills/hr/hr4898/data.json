  "introduced_at": "1990-05-23", 
  "number": "4898", 
  "official_title": "To amend the Act of August 30, 1890 (commonly known as the Second Morrill Act), to remove language purporting to permit racial segregation in land-grant colleges that receive funds under that Act.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
  "sponsor": {
    "district": "5", 
    "name": "Lewis, John", 
