  "introduced_at": "1989-04-11", 
  "number": "1780", 
  "official_title": "To amend title 38, United States Code, to revise the eligibility for outpatient medical services furnished by the Department of Veterans Affairs.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
  "sponsor": {
    "district": "5", 
    "name": "Jontz, Jim", 
