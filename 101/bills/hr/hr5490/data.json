  "introduced_at": "1990-08-03", 
  "number": "5490", 
  "official_title": "To provide for the rehiring of certain former air traffic controllers.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
  "sponsor": {
    "district": "1", 
    "name": "Hochbrueckner, George J.", 
