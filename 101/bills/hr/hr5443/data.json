  "subjects": [
    "AIDS (Disease)", 
    "Grants-in-aid", 
    "Health", 
    "Health education", 
    "Preventive medicine"
  ], 
  "subjects_top_term": "Health", 
  "summary": {
    "as": "Introduced", 
    "date": "1990-08-02", 
    "text": "Amends the Public Health Service Act to modify the formula for determining the minimum amount of allotments to States for acquired immune deficiency syndrome (AIDS) public information activities to allow the amount to be equal to the amount received by the State from the Secretary of Health and Human Services, through the Director of the Centers for Disease Control, for FY 1989 for such activities, if those receipts are greater than the amounts under the alternatives specified by existing law."
  }, 
