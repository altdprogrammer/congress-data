  "introduced_at": "1990-08-01", 
  "number": "5423", 
  "official_title": "To amend the Internal Revenue Code of 1986 to increase the amount of bonds eligible for certain small issuer exceptions, and for other purposes.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
  "sponsor": {
    "district": "4", 
    "name": "Anthony, Beryl, Jr.", 
    "state": "AR", 
