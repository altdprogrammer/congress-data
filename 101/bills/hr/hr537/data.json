  "introduced_at": "1989-01-19", 
  "number": "537", 
  "official_title": "To provide for the establishment of a memorial and museum on Federal land within the District of Columbia to honor members of the Armed Forces who served in World War II and to commemorate the United States participation in that conflict.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
  "sponsor": {
    "district": "9", 
    "name": "Kaptur, Marcy", 
    "state": "OH", 
    "thomas_id": "00616", 
    "title": "Rep", 
    "type": "person"
  }, 
  "status": "REFERRED", 
  "status_at": "1989-01-19", 
  "subjects": [
    "Architecture and the disabled", 
    "Armed forces and national security", 
    "District of Columbia", 
    "Federal advisory bodies", 
    "Government trust funds", 
    "Monuments and memorials", 
