  "status": "PASS_OVER:HOUSE", 
  "status_at": "1989-05-16T15:37:00-04:00", 
  "subjects": [
    "Federal receipts and expenditures", 
    "International affairs", 
    "International agencies", 
    "Latin America", 
    "Palestinians", 
