  "introduced_at": "1973-12-10", 
  "number": "60", 
  "official_title": "A concurrent resolution declaring the existence of a shortage of crude oil, residual fuel oil, and refined petroleum products.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
  "sponsor": {
    "district": null, 
    "name": "Roth Jr., William V.", 
    "state": "DE", 
