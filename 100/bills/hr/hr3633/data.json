  "introduced_at": "1987-11-10", 
  "number": "3633", 
  "official_title": "A bill to require that any United States Government support for military or paramilitary operations in Angola be openly acknowledged and publicly debated.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
