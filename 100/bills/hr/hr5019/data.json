  "introduced_at": "1988-07-12", 
  "number": "5019", 
  "official_title": "A bill to require the Federal Communications Commission to include safeguards for protection of subscribers of small telephone companies in any changes in the method of regulating interstate telephone service rates.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
  "sponsor": {
    "district": "4", 
    "name": "Cooper, Jim", 
    "state": "TN", 
