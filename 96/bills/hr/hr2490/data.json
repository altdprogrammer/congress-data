  "introduced_at": "1979-02-28", 
  "number": "2490", 
  "official_title": "A bill to amend title 18, United States Code, to increase the term of imprisonment for certain offenses relating to carrying or using firearms, to eliminate eligibility for parole with respect to such term, and to require that such term be served before and consecutively to any related sentence of imprisonment.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
  "sponsor": {
    "district": "1", 
    "name": "Heftel, Cecil", 
    "state": "HI", 
    "thomas_id": "00526", 
    "title": "Rep", 
    "type": "person"
  }, 
  "status": "REFERRED", 
  "status_at": "1979-02-28", 
  "subjects": [
    "Corrections and Correctional Institutions", 
    "Crime and law enforcement", 
    "Crime prevention", 
    "Firearms", 
    "Firearms control", 
    "Parole", 
    "Sentences (Criminal procedure)"
  ], 
  "subjects_top_term": "Firearms control", 
  "summary": {
    "as": "Introduced", 
    "date": "1979-02-28", 
    "text": "Requires that whoever uses or carriers a firearm during the commission of any felony which may be prosecuted in a United States court shall, in addition to the punishment provided for the commission of such offense, be sentenced to a term of imprisonment of five years. Requires in the case of a second or subsequent conviction the imposition of a term of eight years imprisonment. Stipulates that the imposition of any term of imprisonment under this section: (1) may not be suspended; (2) may not run concurrently; and (3) may not include parole."
  }, 
  "titles": [
    {
      "as": "introduced", 
      "title": "A bill to amend title 18, United States Code, to increase the term of imprisonment for certain offenses relating to carrying or using firearms, to eliminate eligibility for parole with respect to such term, and to require that such term be served before and consecutively to any related sentence of imprisonment.", 
      "type": "official"
    }
  ], 
