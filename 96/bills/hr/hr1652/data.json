  }, 
  "status": "REFERRED", 
  "status_at": "1979-01-31", 
  "subjects": [
    "Income", 
    "Social welfare", 
    "Supplemental security income program"
  ], 
  "subjects_top_term": "Supplemental security income program", 
  "summary": {
    "as": "Introduced", 
    "date": "1979-01-31", 
    "text": "Amends title XVI (Supplemental Security Income for the Aged, Blind, and Disabled) of the Social Security Act to provide that an individual may work without being considered as having engaged in substantial gainful activity, and thus forfeiting disability status, if such individual's earnings are at a rate that does not exceed the amount specified by this Act. Excludes from an individual's income, for purposes of determining eligibility for benefits under such title, an amount equal to the expenses which such individual incurs in earning income."
  }, 
  "titles": [
    {
      "as": "introduced", 
      "title": "A bill to amend title XVI of the Social Security Act to remove certain work disincentives for the disabled under the supplemental security income benefits program.", 
