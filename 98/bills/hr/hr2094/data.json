  "introduced_at": "1983-03-14", 
  "number": "2094", 
  "official_title": "A bill to amend the Internal Revenue Code of 1954 to allow a credit against tax for expenses incurred in the care of elderly family members.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
  "sponsor": {
    "district": "2", 
    "name": "Tauke, Thomas Joseph", 
    "state": "IA", 
