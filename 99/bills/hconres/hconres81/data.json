  "subjects": [
    "International affairs", 
    "Nicaragua", 
    "Recognition (International law)"
  ], 
  "subjects_top_term": "International affairs", 
  "summary": {
    "as": "Introduced", 
    "date": "1985-03-07", 
    "text": "States that the Congress: (1) reaffirms the Organization of American States declaration that the solution to Nicaragua's political problems is within the exclusive jurisdiction of the Nicaraguan people; (2) urges the President to grant political recognition to the democratic Nicaraguan Resistance; and (3) urges the President and the Organization of American States to support the Resistance."
  }, 
