  "introduced_at": "1985-10-22", 
  "number": "3600", 
  "official_title": "A bill to limit the number of Soviet nationals serving at the Soviet mission to the United Nations, and for other purposes.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
  "sponsor": {
    "district": "4", 
    "name": "Swindall, Patrick L.", 
    "state": "GA", 
