  "introduced_at": "1985-10-10", 
  "number": "3555", 
  "official_title": "A bill to amend the \"Joint Resolution to provide for accepting, ratifying and confirming the cessions of certain islands of the Samoan group to the United States, and for other purposes\".", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
  "sponsor": {
    "district": null, 
    "name": "Sunia, Foto I. F.", 
    "state": "AS", 
    "thomas_id": "01126", 
    "title": "Rep", 
    "type": "person"
  }, 
  "status": "REFERRED", 
  "status_at": "1985-10-10", 
  "subjects": [
    "American Samoa", 
    "Citizenship", 
    "Immigration", 
    "Territories (U.S.)"
  ], 
  "subjects_top_term": "Immigration", 
  "summary": {
    "as": "Introduced", 
    "date": "1985-10-10", 
    "text": "States that a person shall be considered an American national if such person completes 15 years of lawful continuous residence in the United States or American Samoa, and one of his or her parents was a national of the United States and a resident of the United States or American Samoa at the time of that person's birth."
  }, 
