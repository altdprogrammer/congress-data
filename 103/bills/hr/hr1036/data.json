    }, 
    {
      "acted_at": "1993-11-08T16:53:00-05:00", 
      "bill_ids": [
        "hres299-103"
      ], 
      "references": [], 
      "text": "Rules Committee Resolution H. Res. 299 Reported to House. Rule provides for consideration of H.R. 1036 with 1 hour of general debate. Previous question shall be considered as ordered without intervening motions except motion to recommit. Bill is open to amendments. After general debate the bill shall be considered for amendment under the five-minute rule for a period not to exceed four hours.", 
      "type": "action"
    }, 
