  "subjects": [
    "Congress and Congressmen", 
    "Congressional salaries and pensions", 
    "Members of Congress"
  ], 
  "subjects_top_term": "Congressional salaries and pensions", 
  "summary": {
    "as": "Introduced", 
    "date": "1981-10-21", 
    "text": "Amends the Legislative Reorganization Act of 1946 to provide that any adjustment in the rate of pay for Members of Congress proposed during any Congress shall not take effect earlier than the beginning of the next Congress. States that any such pay adjustment proposed in an even-numbered year of any Congress after the congressional elections and before the beginning of the following Congress shall be considered as occurring during the first session of the following Congress for the purposes of this Act."
  }, 
