  "status_at": "1981-10-13", 
  "subjects": [
    "Corrections and Correctional Institutions", 
    "Old age, survivors and disability insurance", 
    "Prisoners", 
    "Social security eligibility"
  ], 
  "subjects_top_term": "Old age, survivors and disability insurance", 
  "summary": {
    "as": "Introduced", 
    "date": "1981-10-13", 
    "text": "Amends title II (Old Age, Survivors and Disability Insurance) of the Social Security Act to prohibit the payment of title II benefits to an incarcerated felon during any month of confinement. Treats the benefits withheld as having been paid to the felon for purposes of the payment of benefits to other persons so entitled on the basis of the wages and self-employment income of the felon."
  }, 
