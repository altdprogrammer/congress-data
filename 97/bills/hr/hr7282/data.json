  "introduced_at": "1982-10-01", 
  "number": "7282", 
  "official_title": "A bill to require the Administrator of General Services to take certain actions to improve the management of Federal procurement programs.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
  "sponsor": {
    "district": "2", 
    "name": "Schneider, Claudine", 
