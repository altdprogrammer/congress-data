  "subjects": [
    "Income", 
    "Old age, survivors and disability insurance"
  ], 
  "subjects_top_term": "Old age, survivors and disability insurance", 
  "summary": {
    "as": "Introduced", 
    "date": "1981-01-27", 
    "text": "Amends title II (Old Age, Survivors and Disability Insurance) of the Social Security Act to increase the amount of outside earnings permitted each year without any deduction from OASDI benefits by an amount equal to the total of all State and local real property taxes which an individual pays during a taxable year."
  }, 
