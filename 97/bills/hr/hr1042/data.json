  "introduced_at": "1981-01-22", 
  "number": "1042", 
  "official_title": "A bill to provide an opportunity for taxpayers to make financial contributions, in connection with the payment of their Federal income tax, for the advancement of the arts.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
  "sponsor": {
    "district": "14", 
    "name": "Richmond, Frederick W.", 
    "state": "NY", 
