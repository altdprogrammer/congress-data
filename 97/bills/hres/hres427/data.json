  "introduced_at": "1982-04-01", 
  "number": "427", 
  "official_title": "A resolution expressing the sense of the House of Representatives with respect to the issuance of mineral leases in wilderness areas, and for other purposes.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
  "sponsor": {
    "district": "2", 
    "name": "Udall, Morris K.", 
    "state": "AZ", 
    "thomas_id": "01173", 
    "title": "Rep", 
    "type": "person"
  }, 
  "status": "REFERRED", 
  "status_at": "1982-04-01", 
  "subjects": [
    "Mines and mineral resources", 
    "Mining leases", 
    "Parks and Natural Areas", 
    "Wilderness areas"
  ], 
  "subjects_top_term": "Wilderness areas", 
  "summary": {
    "as": "Introduced", 
    "date": "1982-04-01", 
    "text": "Expresses the sense of the House of Representatives that: (1) the Secretary of the Interior should refrain from issuing mineral leases in wilderness areas and lands under formal consideration for wilderness designation; (2) any Federal official who proposes to issue a mineral lease in such an area should provide prior notice to Congress in order to allow congressional prohibition of such lease; (3) additions to the Wilderness System should proceed expeditiously but without arbitrary deadlines; (4) lands under formal consideration for wilderness designation should be managed to preserve their wilderness character; and (5) Federal land management agencies should consider wilderness values in making land management decisions and recommendations."
  }, 
