  "subjects_top_term": "Armed forces reserves", 
  "summary": {
    "as": "Introduced", 
    "date": "1981-12-08", 
    "text": "Expresses the sense of Congress that members of the National Guard and Reserve should be granted time off from their jobs for military training, exclusive of earned vacation, and given equal consideration for job benefits and promotions as other employees."
  }, 
  "titles": [
    {
      "as": "introduced", 
